{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": ["next/core-web-vitals", "prettier"],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
      "quotes": ["error", "double"],
      "next/core-web-vitals": "off"
  }
}